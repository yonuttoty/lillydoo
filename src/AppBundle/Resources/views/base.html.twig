<!DOCTYPE html>
<html lang="en">
<head>
    <title>Homepage</title>
    {% javascripts
        'public/js/bootstrap/jquery.min.js'
        'public/js/bootstrap/popper.min.js'
        'public/js/bootstrap/bootstrap.min.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% block stylesheets %}
        <link href="{{ asset('public/css/bootstrap/bootstrap.min.css') }}" rel="stylesheet"/>
        <link href="{{ asset('public/css/main.css') }}" rel="stylesheet"/>
        <link href="{{ asset('public/css/title_bar.css') }}" rel="stylesheet"/>
        <link href="{{ asset('public/css/wrapper.css') }}" rel="stylesheet"/>
        <link href="{{ asset('public/css/address.css') }}" rel="stylesheet"/>
        <link href="{{ asset('public/css/item.css') }}" rel="stylesheet"/>
    {% endblock %}
</head>
<body>
<div>
    <div class="title-bar">
        <div class="wrapper"><h1>Lilydoo Coding Challenge</h1>
            <h2 class="title-bar__subtitle">Created by Ioan</h2></div>
    </div>
    <div class="wrapper">
        {% block content %}
        {% endblock %}
    </div>
</div>
</body>
</html>
